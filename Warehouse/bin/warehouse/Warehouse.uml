        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_nQ8RH7Y-Eeu76qlWFzZctA" name="indexProcessing" specification="_nQSw0LY-Eeu76qlWFzZctA">
          <ownedParameter xmi:id="_nQ8RILY-Eeu76qlWFzZctA" name="driverDet" type="_nPlmMbY-Eeu76qlWFzZctA"/>
          <ownedParameter xmi:id="_nQ8RIbY-Eeu76qlWFzZctA" name="nameToBeRemoved">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_nQ8RIrY-Eeu76qlWFzZctA" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>// TODO Auto-generated method stub&#xD;
		int index = 0;&#xD;
		int counter = 0;&#xD;
		// System.out.println(&quot;Available names are:\n&quot;);&#xD;
		for (int i = 0; i &lt; driverDet.size(); i++) {&#xD;
&#xD;
			if (driverDet.get(i).getDriverName().toLowerCase().equals(nameToBeRemoved.toLowerCase())) {&#xD;
				index = i;&#xD;
				break;&#xD;
			}&#xD;
			counter = i + 1;&#xD;
		}&#xD;
		if (counter == driverDet.size()) {&#xD;
			// No matching name is found, return index=-1&#xD;
			index = -1;&#xD;
		}&#xD;
		return index;&#xD;
	</body>
