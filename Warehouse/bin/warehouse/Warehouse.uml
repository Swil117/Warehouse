        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_nQ6b4LY-Eeu76qlWFzZctA" name="_initializer1" specification="_nNR5ELY-Eeu76qlWFzZctA">
          <language>java</language>
          <body>String path = &quot;Data Files\\&quot;;&#xD;
